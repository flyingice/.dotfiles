[core]
	editor = vim
	excludesFile = ~/.gitignore_global
	pager = diff-so-fancy | less --tabs=4 -RFX
[merge]
	tool = vimdiff
[diff]
	tool = vimdiff
[alias]
	graph = log --all --graph --decorate --oneline
	ctags = !.git/hooks/ctags
[init]
	templatedir = ~/.git_template
[commit]
	gpgsign = true
